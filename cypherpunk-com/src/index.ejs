    .btn.blue { background-color: #1987bd; }
    .btn.teal { background-color: #163138; }
    .btn.orange { background-color: #ff7857; }
    .btn:disabled { cursor: default; background-color: #ccc; }
    input:disabled { background-color: #eee !important; }

    .down-arrow { position: relative; }
    .down-arrow.green:after { border-top: 20px solid #163238; }
    .down-arrow.beige:after { border-top: 20px solid #efeaea; }
    .down-arrow:after {
      content: ' ';
      width: 0;
      height: 0;
      border-left: 20px solid transparent;
      border-right: 20px solid transparent;
      border-top: 20px solid #fff;
      position: absolute;
      bottom: -20px;
      left: 0;
      right: 0;
      margin: auto;
      z-index: 2;
    }

    .anchor-target {
      padding-top: 80px;
      margin-top: -80px;
      width: 1px;
      display: inline-block;
    }

    .jeff img { display: none; }

    .article .separator { padding: 20px 0; }
    .article .content img { width: 100%; height: 100%; max-width: 100%; }

    .modal {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      opacity: 0;
      visibility: hidden;
      overflow: auto;
      z-index: 500;
      transition: opacity 0.5s;
    }
    .modal.open { visibility: visible; opacity: 1; }

    .modal .background {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      display: none;
      background-color: rgba(0, 0, 0, 0.35);
    }
    .modal .background.open { display: block; }

    .modal .modal-content {
      position: relative;
      width: 100%;
      max-width: 500px;
      padding: 25px;
      margin: 10% auto;
      border-radius: 15px;
      background-color: #fff;
      box-shadow: -5px 5px 2px #163138;
    }

    .modal .modal-content .modal-header {
      width: 100%;
      padding: 15px 0 0;
      color: #28434E;
      font-size: 28px;
      font-weight: 500;
      letter-spacing: 0.6px
    }

    .modal .modal-content .modal-line {
      width: 25px;
      height: 2px;
      margin: 10px 0 15px 0;
      background-color: #61BBC3;
    }

    .modal .modal-content .modal-body .row { text-align: left; margin: 0 0 15px; }

    .modal .modal-content .modal-body .row .countdown {
      color: #61BBC3;
      text-align: center;
      font-size: 40px;
    }

    .modal .modal-content .modal-body .row label {
      font-size: 18px;
      font-weight: 500;
      color: #61BBC3;
    }

    .modal .modal-content .modal-body .row input {
      width: 100%;
      height: 40px;
      border-radius: 5px;
      border: solid 1px #61BBC3;
      color: #163439;
      font-size: 18px;
      font-weight: 500;
      padding: 10px;
      margin: 5px 0 0;
    }
    .modal .modal-content .modal-body .row input.error {
      border-color: #a94442;
      box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 10px #ce8483;
    }

    .modal .modal-content .modal-body .row textarea { width: 100%; height: 75px; }
    .modal .modal-content .modal-body .row textarea.error {
      border-color: #a94442;
      box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 10px #ce8483;
    }

    .modal .modal-content .modal-footer {
      margin: 25px 0 0;
      text-align: right;
    }

    .modal .modal-content .modal-footer .btn { width: 130px; height: 41px; }

    @media (max-width: 1060px) {
      .modal .modal-content { width: 90%; }
      .modal .modal-content .modal-footer .btn { width: 49%; }
    }

    @media (max-width: 1000px) {
      .wrapper { margin-bottom: -847px; }
      .footerContainer, .wrapper:after { height: 847px;}
    }
