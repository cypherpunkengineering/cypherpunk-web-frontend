    .down-arrow.gray:after { border-top: 20px solid #efebeb; }
    .down-arrow:after {
      content: ' ';
      width: 0;
      height: 0;
      border-left: 20px solid transparent;
      border-right: 20px solid transparent;
      border-top: 20px solid #fff;
      position: absolute;
